{
  "name": "sastest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "ENVFILE=.env react-native run-android",
    "win-android:dev": "SET ENVFILE=.env && react-native run-android",
    "release:dev": "cd android && ENVFILE=.env ./gradlew app:assembleRelease",
    "win-release:dev": "cd android && SET ENVFILE=.env && ./gradlew app:assembleRelease",
    "install-release": "cd android && adb install app/build/outputs/apk/release/app-release.apk",
    "ios": "ENVFILE=.env react-native run-ios",
    "ios-release:dev": "ENVFILE=.env react-native run-ios --configuration=release",
    "xcclean": "cd ios && ENVFILE=.env xcodebuild clean -scheme opi -workspace opi.xcworkspace",
    "gc": "cd android && ENVFILE=.env ./gradlew clean",
    "win-gc": "cd android && SET ENVFILE=.env && gradlew clean",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pod": "cd ios && npx pod-install",
    "rl": "rm -rf node_modules && yarn &&yarn pod && yarn ios && yarn gc && yarn android:dev"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-native": "0.64.1",
    "react-native-dotenv": "^2.5.5",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.3.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-vector-icons": "^8.1.0",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.2.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-module-resolver": "^1.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.3.1",
    "react-test-renderer": "17.0.1",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
